{
    "name": "vscodeex",
    "displayName": "vscodeEx",
    "description": "",
    "version": "0.0.1",
    "publisher": "testEx",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:plaintext",
        "onCommand:catCoding.start"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands":[
            {
                "command": "catCoding.start",
                "title": "Start new cat doding session",
                "category": "Cat Coding"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "configuration":{
        "type":"object",
        "title":"Example configuration",
        "properties":{
            "languageServerExample.maxNumberOfProblems":{
                "scope":"resource",
                "type":"number",
                "default":100,
                "description":"Controls the maxinum number of problems produced by the server."
            }
        }
    },
    "dependencies": {
        "vscode":"^1.1.26",
        "vscode-languageclient":"^4.14"
    }
}
